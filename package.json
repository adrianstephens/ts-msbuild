{
	"name": "@isopodlabs/msbuild",
	"version": "1.0.0",
	"description": "A package for reading and running MSBuild files with TypeScript.",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/adrianstephens/ts-msbuild.git"
	},
	"main": "dist/index.js",
	"files": [
		"dist/**/*.{js,d.ts}",
		"README.md"
	],
	"scripts": {
		"build": "tsc",
		"watch": "tsc -watch -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"patch": "ts-patch install"
	},
	"keywords": [
		"msbuild",
		"parser",
		"build",
		"typescript",
		"nodejs"
	],
	"author": "Adrian Stephens",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^22.13.8",
		"eslint": "^9.21.0",
		"ts-node": "^10.9.2",
		"ts-patch": "^3.3.0",
		"typescript-eslint": "^8.26.0"
	},
	"dependencies": {
		"@isopodlabs/binary": "^1.6.0",
		"@isopodlabs/binary_libs": "^1.3.0",
		"@isopodlabs/registry": "^0.1.0",
		"@isopodlabs/utilities": "^1.9.0",
		"@isopodlabs/xml": "^1.2.0"
	}
}
